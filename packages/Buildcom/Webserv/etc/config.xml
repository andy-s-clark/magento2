<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modules>
		<Buildcom_Webserv>
			<version>0.0.1.0</version>
			<active>true</active>
		</Buildcom_Webserv>
	</modules>
	<frontend>
		<routers>
			<webserv>
				<use>standard</use>
				<args>
					<module>Buildcom_Webserv</module>
					<frontName>webserv</frontName>
				</args>
			</webserv>
		</routers>
	</frontend>
	<global>
	   <events>
            <webserv_product_load_after>
                <observers>
                    <inventory>
                        <class>Buildcom_Webserv_Model_Observer</class>
                        <method>addInventoryData</method>
                    </inventory>
                </observers>
            </webserv_product_load_after>
        </events>
    </global>
    <default>
        <webserv>
            <server>
                <production>http://webservices.sys.id.build.com:8080/build-webservices-1.0.0/services</production>
                <development>http://devbox2.build.internal:8080/build-webservices-1.0.0/services</development>
                <active>development</active>
            </server>
        </webserv>
    </default>
</config>